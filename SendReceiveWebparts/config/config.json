{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "sender-webpart-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/senderWebpart/SenderWebpartWebPart.js",
          "manifest": "./src/webparts/senderWebpart/SenderWebpartWebPart.manifest.json"
        }
      ]
    },
    "receive-webpart-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/receiveWebpart/ReceiveWebpartWebPart.js",
          "manifest": "./src/webparts/receiveWebpart/ReceiveWebpartWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "SenderWebpartWebPartStrings": "lib/webparts/senderWebpart/loc/{locale}.js",
    "ReceiveWebpartWebPartStrings": "lib/webparts/receiveWebpart/loc/{locale}.js"
  }
}
